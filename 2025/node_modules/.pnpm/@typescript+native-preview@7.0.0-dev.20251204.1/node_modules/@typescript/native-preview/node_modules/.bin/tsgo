#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tom/Projects/adventofcode/2025/node_modules/.pnpm/@typescript+native-preview@7.0.0-dev.20251204.1/node_modules/@typescript/native-preview/bin/node_modules:/Users/tom/Projects/adventofcode/2025/node_modules/.pnpm/@typescript+native-preview@7.0.0-dev.20251204.1/node_modules/@typescript/native-preview/node_modules:/Users/tom/Projects/adventofcode/2025/node_modules/.pnpm/@typescript+native-preview@7.0.0-dev.20251204.1/node_modules/@typescript/node_modules:/Users/tom/Projects/adventofcode/2025/node_modules/.pnpm/@typescript+native-preview@7.0.0-dev.20251204.1/node_modules:/Users/tom/Projects/adventofcode/2025/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/tom/Projects/adventofcode/2025/node_modules/.pnpm/@typescript+native-preview@7.0.0-dev.20251204.1/node_modules/@typescript/native-preview/bin/node_modules:/Users/tom/Projects/adventofcode/2025/node_modules/.pnpm/@typescript+native-preview@7.0.0-dev.20251204.1/node_modules/@typescript/native-preview/node_modules:/Users/tom/Projects/adventofcode/2025/node_modules/.pnpm/@typescript+native-preview@7.0.0-dev.20251204.1/node_modules/@typescript/node_modules:/Users/tom/Projects/adventofcode/2025/node_modules/.pnpm/@typescript+native-preview@7.0.0-dev.20251204.1/node_modules:/Users/tom/Projects/adventofcode/2025/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/tsgo.js" "$@"
else
  exec node  "$basedir/../../bin/tsgo.js" "$@"
fi
